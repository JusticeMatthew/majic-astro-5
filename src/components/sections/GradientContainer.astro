---
import { Image } from "astro:assets";
import mobileBg from "$/assets/images/mobile-bg.png";
---

<Image
  src={mobileBg}
  width={mobileBg.width}
  alt=""
  class="fixed top-0 left-0 z-0 h-screen w-screen object-cover object-center"
  loading="eager"
  fetchpriority="high"
  format="webp"
  quality={80}
/>
